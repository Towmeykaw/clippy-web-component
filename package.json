{
  "name": "clippy-web-component",
  "version": "0.0.4",
  "description": "A Clippy web component",
  "main": "dist/clippy.js",
  "module": "dist/clippy.js",
  "type": "module",
  "scripts": {
    "build": "tsc --watch -p tsconfig.dev.json",
    "compile": "tsc -p tsconfig.prod.json",
    "lint": "lit-analyzer && eslint 'src/**/*.ts'",
    "format": "prettier src/* --write",
    "serve": "es-dev-server --node-resolve --watch"
  },
  "keywords": [
    "web component",
    "lit-element",
    "typescript",
    "clippy"
  ],
  "author": "Towmeykaw",
  "license": "MIT",
  "dependencies": {
    "lit-element": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Towmeykaw/clippy-web-component"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "es-dev-server": "^1.56.0",
    "eslint": "^7.3.1",
    "lit-analyzer": "^1.1.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "typescript-lit-html-plugin": "^0.9.0"
  }
}
